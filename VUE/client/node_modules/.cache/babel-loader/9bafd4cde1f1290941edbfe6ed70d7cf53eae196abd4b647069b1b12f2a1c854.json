        }\n        this.initialized = 1;\n      });\n    },\n    ChangeNodedata: function () {\n      var filename = \"cc-cc408-hya\" + '/' + this.nodefeature + '.csv';\n      this.xData2 = [];\n      this.yData2 = [];\n      axios.get(filename).then(res => {\n        var value = d3.csvParse(res.data);\n        for (var i = 0; i < value.length; i++) {\n          if (value[i].node_name + \"(\" + value[i].node_ip + \")\" == this.nodechoose) {\n            this.xData2.push(Number((value[i].timestamp - value[0].timestamp) / 60));\n            this.yData2.push(Number(value[i].value));\n          }\n        }\n        this.flag2 = 1;\n      });\n      console.log(this.xData2);\n    }\n  }\n};","map":{"version":3,"names":["axios","echarts","d3","require","data","nodelist","name","id","node_feature_list","myChart2","xData2","yData2","flag2","nodechoose","nodefeature","initialized","myChartStyle","float","width","height","mounted","initEcharts2","watch","methods","option","dataZoom","type","show","left","right","bottom","start","end","filterMode","xAxisIndex","textStyle","color","fontSize","orient","xAxis","yAxis","series","showSymbol","tooltip","trigger","backgroundColor","extraCssText","padding","axisPointer","lineStyle","x","y","x2","y2","colorStops","offset","global","grid","containLabel","top","graphic","LinearGradient","borderColor","init","document","getElementById","setOption","window","addEventListener","resize","ChooseNode","event","target","value","ChangeNodedata","Choose_Node_Feature","Initdata","filename","get","then","res","csvParse","i","length","node_name","node_ip","push","Number","timestamp","console","log"],"sources":["E:\\VUE\\frontend\\src\\components\\NodeChart1.vue"],"sourcesContent":["<template>\r\n    <div class=\"node\">\r\n      <div class=\"title\">cc-cc408-hya节点单指标</div> \r\n    <select v-model=\"nodechoose\" style=\"width:100px;height:30px;margin-top:5px;margin-bottom:5px;margin-left:10px\"\r\n      @change=\"ChooseNode($event)\">\r\n      <opti