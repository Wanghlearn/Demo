ode(event)\r\n    {\r\n      this.nodename=event.target.value\r\n      if(this.node_feature_name!=\"\")\r\n      this.ChangeNodedata()\r\n    },\r\n    Choose_Node_Feature(event)\r\n    {\r\n      this.node_feature_name=event.target.value\r\n      //console.log(this.node_feature_name)\r\n      if(this.node_name!=\"\")\r\n      this.ChangeNodedata()\r\n    },\r\n    \r\n    ChangeNodedata:function()\r\n    {\r\n      var filename=\"cc-cc408-hya\"+'/'+this.node_feature_name+'.csv'\r\n      this.xData2=[]\r\n      this.yData2=[]\r\n      axios.get(filename)\r\n      .then(res=>{\r\n        var value=d3.csvParse(res.data)\r\n        for(var i=0;i<value.length;i++)\r\n        {\r\n          if(value[i].node_name==this.nodename)\r\n          {\r\n            this.xData2.push(Number((value[i].timestamp-value[0].timestamp)/60))\r\n            this.yData2.push(Number(value[i].value))\r\n          }\r\n          \r\n        }\r\n        this.flag2=1\r\n      })\r\n    }\r\n    }\r\n}\r\n</script>"],"mappings":";;EACSA,KAAK,EAAC;AAAM;gCAGfC,mBAAA,CAAmC;EAA3BC,KAAK,EAAC;AAAE,GAAC,WAAS;;gCAK1BD,mBAAA,CAAqC;EAA7BC,KAAK,EAAC;AAAE,GAAC,aAAW;;;uBAR9BC,mBAAA,CAYI,OAZJC,UAYI,G,gBAXJH,mBAAA,CAIS;+DAJQI,IAAA,CAAAC,UAAU,GAAAC,MAAA;IAAEC,KAAiF,EAAjF;MAAA;MAAA;MAAA;MAAA;MAAA;IAAA,CAAiF;IAC3GC,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAAAH,MAAA,IAAEI,QAAA,CAAAC,UAAU,CAACL,MAAM;MAC1BM,UAAmC,G,kBACnCV,mBAAA,CAAkFW,SAAA,QAAAC,WAAA,CAA3DC,KAAA,CAAAC,QAAQ,EAAhBC,IAAI;yBAAnBf,mBAAA,CAAkF;MAA1CgB,GAAG,EAAED,IAAI,CAACE,EAAE;mBAAEC,gBAAA,CAAQH,IAAI,CAACI,IAAI;;2FAHxDjB,IAAA,CAAAC,UAAU,E,mBAK3BL,mBAAA,CAIS;+DAJQI,IAAA,CAAAkB,WAAW,GAAAhB,MAAA;IAAEC,KAAiF,EAAjF;MAAA;MAAA;MAAA;MAAA;MAAA;IAAA,CAAiF;IAC5GC,QAAM,EAAAC,MAAA,QAAAA,MAAA,MAAAH,MAAA,IAAEI,QAAA,CAAAa,mBAAmB,CAACjB,MAAM;MACnCkB,UAAqC,G,kBACrCtB,mBAAA,CAA2FW,SAAA,QAAAC,WAAA,CAApEC,KAAA,CAAAU,iBAAiB,EAAzBR,IAAI;yBAAnBf,mBAAA,CAA2F;MAA1CgB,GAAG,EAAED,IAAI,CAACE,EAAE;mBAAEC,gBAAA,CAAQH,IAAI,CAACI,IAAI;;2FAHjEjB,IAAA,CAAAkB,WAAW,E,GAK3BtB,mBAAA,CAA8D;IAAzDD,KAAK,EA