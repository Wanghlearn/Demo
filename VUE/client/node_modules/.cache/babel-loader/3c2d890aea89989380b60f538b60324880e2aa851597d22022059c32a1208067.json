{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport * as echarts from \"echarts\";\nconst d3 = require('d3-dsv');\nexport default {\n  data() {\n    return {\n      clusterlist: [{\n        name: \"cc-cc408-hya\",\n        id: 1\n      }, {\n        name: \"cc-cc553\",\n        id: 2\n      }],\n      featurelist: [{\n        name: \"status\",\n        id: 1\n      }, {\n        name: \"active_shards\",\n        id: 2\n      }, {\n        name: \"health_node\",\n        id: 3\n      }],\n      myChart: {},\n      xData: [],\n      yData: [],\n      myChartStyle: {\n        float: \"left\",\n        width: \"100%\",\n        height: \"85%\"\n      },\n      flag: 0,\n      cluster: \"cc-cc408-hya\",\n      featurename: \"status\",\n      clusterchoose: \"cc-cc408-hya\",\n      featurechoose: \"status\"\n    };\n  },\n  beforeMount() {\n    this.Initdata();\n  },\n  mounted() {\n    this.initEcharts();\n  },\n  watch: {\n    flag: function () {\n      this.flag = 0;\n      this.initEcharts();\n    }\n  },\n  methods: {\n    initEcharts() {\n      const option = {\n        dataZoom: [{\n          type: 'inside',\n          height: 20,\n          left: '10%',\n          right: '10%',\n          bottom: '2%',\n          start: 90,\n          end: 100,\n          filterMode: \"filter\",\n          xAxisIndex: [0],\n          textStyle: {\n            color: \"#71A3CD\",\n            fontSize: 11\n          }\n        }, {\n          type: \"inside\",\n          orient: \"vertical\"\n        }],\n        xAxis: {\n          data: this.xData\n\n          //type:\"time\"\n        },\n\n        yAxis: {},\n        series: [{\n          data: this.yData,\n          type: \"line\",\n          showSymbol: false\n        }],\n        color: [\"rgba(21,75,200,0.9)\"],\n        tooltip: {\n          trigger: \"axis\",\n          backgroundColor: 'rgba(21,75,140,0.9)',\n          extraCssText: 'box-shadow: 0  6px 10px rgba(0,8,23,.65)',\n          padding: [5, 20],\n          axisPointer: {\n            type: \"cross\",\n            lineStyle: {\n              width: '40',\n              color: {\n                type: 'linear',\n                x: 0,\n                y: 0,\n                x2: 0,\n                y2: 1,\n                colorStops: [{\n                  offset: 0,\n                  color: 'rgba(91,179,252,0.5)' // 0% 处的颜色\n                }, {\n                  offset: 1,\n                  color: 'rgba(91,179,252,1)' // 100% 处的颜色\n                }],\n\n                global: false // 缺省为 false\n              }\n            }\n          }\n        },\n\n        grid: {\n          //坐标系设置\n          containLabel: true,\n          left: 0,\n          right: 10,\n          bottom: 0,\n          top: 34,\n          show: true,\n          backgroundColor: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n            offset: 0,\n            color: \"rgba(1,48,87,0.2)\" // 0% 处的颜色\n          }, {\n            offset: 1,\n            color: \"rgba(1,48,87,0.8)\" // 100% 处的颜色\n          }]),\n\n          //背景渐变色\n          borderColor: \"rgba(0,0,0,0)\"\n        }\n      };\n      this.myChart = echarts.init(document.getElementById(\"mychart\"));\n      this.myChart.setOption(option);\n      window.addEventListener(\"resize\", () => {\n        this.myChart.resize();\n      });\n    },\n    ChooseCluster(event) {\n      this.cluster = event.target.value;\n      if (this.featurename != \"\") {\n        this.Changedata();\n      }\n    },\n    ChooseFeature(event) {\n      this.featurename = event.target.value;\n      this.Changedata();\n    },\n    Initdata() {\n      //console.log(1111)\n      var filename = this.clusterchoose + '/' + this.featurechoose + '.csv';\n      axios.get(filename).then(res => {\n        var value = d3.csvParse(res.data);\n        for (var i = 0; i < value.length; i++) {\n          this.xData.push(Number((value[i].timestamp - value[0].timestamp) / 60));\n          this.yData.push(Number(value[i].value));\n          //this.flag=1\n        }\n      });\n      ///console.log(11111)\n    },\n\n    Changedata: function () {\n      var filename = this.cluster + '/' + this.featurename + '.csv';\n      this.xData = [];\n      this.yData = [];\n      axios.get(filename).then(res => {\n        var value = d3.csvParse(res.data);\n        for (var i = 0; i < value.length; i++) {\n          this.xData.push(Number((value[i].timestamp - value[0].timestamp) / 60));\n          this.yData.push(Number(value[i].value));\n          this.flag = 1;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","echarts","d3","require","data","clusterlist","name","id","featurelist","myChart","xData","yData","myChartStyle","float","width","height","flag","cluster","featurename","clusterchoose","featurechoose","beforeMount","Initdata","mounted","initEcharts","watch","methods","option","dataZoom","type","left","right","bottom","start","end","filterMode","xAxisIndex","textStyle","color","fontSize","orient","xAxis","yAxis","series","showSymbol","tooltip","trigger","backgroundColor","extraCssText","padding","axisPointer","lineStyle","x","y","x2","y2","colorStops","offset","global","grid","containLabel","top","show","graphic","LinearGradient","borderColor","init","document","getElementById","setOption","window","addEventListener","resize","ChooseCluster","event","target","value","Changedata","ChooseFeature","filename","get","then","res","csvParse","i","length","push","Number","timestamp"],"sources":["E:\\VUE\\frontend\\src\\components\\ClusterChart.vue"],"sourcesContent":["<template>\r\n    <div class=\"cluster\">\r\n        <div class=\"title\">集群层面</div> \r\n<select v-model=\"clusterchoose\" style=\"width:100px;height:30px;margin-top:5px;margin-bottom:5px;margin-left:10px\"\r\n            @change=\"ChooseCluster($event)\">\r\n  <option value=\"\">请选择所要展示集群</option>\r\n  <option v-for=\"item in clusterlist\" v-bind:key=\"item.id\" v-tex