{"ast":null,"code":"import axios from \"axios\";\nimport * as echarts from \"echarts\";\nexport default {\n  data() {\n    return {\n      xData: [],\n      yData: [],\n      flag: 0,\n      myChartStyle: {\n        float: \"left\",\n        width: \"100%\",\n        height: \"85%\"\n      },\n      myChart: {}\n    };\n  },\n  created() {\n    this.initData();\n  },\n  mounted() {\n    this.initEcharts();\n  },\n  watch: {\n    flag: function () {\n      this.flag = 0;\n      this.initEcharts();\n    }\n  },\n  methods: {\n    initData() {\n      axios.get(\"http://localhost:5000/similar\").then(res => {\n        var value = res.data[1];\n        this.xData = value[4][0];\n        this.yData = value[4][1];\n        this.flag = 1;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    initEcharts() {\n      const option = {\n        dataZoom: [{\n          type: 'inside',\n          height: 20,\n          left: '10%',\n          right: '10%',\n          bottom: '2%',\n          start: 90,\n          end: 100,\n          filterMode: \"filter\",\n          xAxisIndex: [0],\n          textStyle: {\n            color: \"#71A3CD\",\n            fontSize: 11\n          }\n        }, {\n          type: \"inside\",\n          orient: \"vertical\"\n        }],\n        xAxis: {\n          data: this.xData\n\n          //type:\"time\"\n        },\n\n        yAxis: {},\n        series: [{\n          data: this.yData,\n          type: \"line\",\n          showSymbol: false,\n          itemStyle: {\n            borderRadius: 30\n          }\n        }],\n        color: [\"rgba(21,75,200,0.9)\"],\n        tooltip: {\n          trigger: \"axis\",\n          backgroundColor: 'rgba(21,75,140,0.9)',\n          extraCssText: 'box-shadow: 0  6px 10px rgba(0,8,23,.65)',\n          padding: [5, 20],\n          axisPointer: {\n            type: \"cross\",\n            lineStyle: {\n              width: '40',\n              color: {\n                type: 'linear',\n  